{
     "manifest_version": 2,
     "name": "ActTab",
     "version": "0.1",
     "background": {
          "scripts": ["background.js"],
          "persistent": false
        },
     "permissions":["tabs", "declarativeContent"],
     "content_scripts": [
         {
              "matches": ["https://syosetu.com/*"],
              "js": ["jquery-3.5.1.min.js", "alert.js"]
         }
    ]
}