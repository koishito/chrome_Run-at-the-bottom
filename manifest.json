{
     "manifest_version": 2,
     "name": "Run at the bottom",
     "version": "0.1",
     "permissions":[
          "storage",
          "unlimitedStorage",
          "tabs",
          "declarativeContent"
     ],
     "options_page": "html/options.html",
     "browser_action": {
          "default_popup": "html/options.html"
     },
     "background": {
          "scripts": ["js/background.js"],
          "persistent": false
     },

     "content_scripts": [
          {
            "matches": [
              "file:///*"
            ],
            "js": [
              "js/contents.js"
            ]
          }
     ]
     
}