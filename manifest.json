{
     "manifest_version": 2,
     "name": "Run at the bottom",
     "version": "1.1",
     "permissions":[
          "storage",
          "unlimitedStorage",
          "tabs",
          "notifications",
          "http://*/*",
          "https://*/*",
          "declarativeContent"
     ],
     // "content_scripts": [
     //      {
     //        "matches": ["file://*.txt/"],
     //        "js": ["js/content-script.js"]
     //      }
     //    ],
     "options_page": "html/options.html",
     "background": {
          "scripts": ["js/background.js"],
          "persistent": false
     },
     "browser_action": {
            "default_popup":"html/options.html"
        }
}